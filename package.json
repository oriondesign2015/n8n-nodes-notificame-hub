{
  "name": "n8n-nodes-notificame-hub",
  "version": "0.1.8",
  "description": "A NotificaMe Hub automatiza a comunicação em múltiplos canais, oferecendo soluções integradas e escaláveis.",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "OrionDesign",
    "email": "contato@oriondesign.art.br"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oriondesign2015/n8n-nodes-notificame-hub.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "build-no-lint": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/NotificaMeHubApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/NotificaMeHub/NotificaMeHub.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.18.0",
    "braces": "3.0.3",
    "decode-uri-component": "0.2.1",
    "es5-ext": "0.10.63",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^4.0.2",
    "micromatch": "4.0.8",
    "prettier": "^3.5.3",
    "semver": "5.7.2",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "n8n-workflow": "^1.82.0",
    "sqlite3": "^5.1.7"
  },
  "resolutions": {
    "braces": "^3.0.3",
    "micromatch": "^4.0.8"
  }
}
